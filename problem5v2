  for x in list:
    if x % 12 is 0:
      list12.append(x)
    else: 
      continue
    
  for x in list:
    if x % 11 is 0:
      list11.append(x)
    else: 
      continue
    
createlists()
    
list19 = list19[::-1]

def checklists():
  for n in list19:
    if n in list17:
      if n  in list13:
        if n  in list11:
          if n  in list20:
            if n  in list18:
              if n  in list16:
                if n  in list15:
                  if n  in list14:
                    if n  in list12:
                      print "sucess: ", n
                      pause = raw_input("stoploop")
                    else:
                      continue
  print "no match found"

checklists()
